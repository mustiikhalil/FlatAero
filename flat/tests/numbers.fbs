enum Values: int {
    l1 = 0, l2, l3
}

struct Position {
    x: float;
    y: float;
    z: float;
}

table PIValue {
    name: string;
}

table Numbers {
    values1: Values;
    values2: Values;
    n1: byte;
    n2: ubyte;
    n3: bool;
    n4: short;
    n5: ushort;
    n6: int;
    n7: uint;
    n8: long;
    n9: ulong;
    n10: float;
    n11: double;
    p: Position;
    pi: PIValue;
}

root_type Numbers;